{
	"name" : "sunhill/collection",
	"description" : "The sunhill collection of useful objects, marketeers and views",
	"type" : "library",
	"license" : "MIT",
	"authors" : [{
			"name" : "Klaus Dimde",
			"email" : "klaus@dimde.info"
		}
	],
	"minimum-stability" : "dev",
        "repositories" : [{
                        "type" : "path",
                        "url" : "../basic",
                        "options" : {
                                "symlink" : true
                        }
                },{
                        "type" : "path",
                        "url" : "../orm",
                        "options" : {
                                "symlink" : true
                        }
                },{
                        "type" : "path",
                        "url" : "../visual",
                        "options" : {
                                "symlink" : true
                        }
                }
                
                ],
	"require": {
		"sunhill/basic":"*", 
		"sunhill/orm":"*",
		"sunhill/infomarket":"*",
		"sunhill/visual":"*"
	},
        "require-dev" : {
                "phpunit/phpunit" : "^9.5.10",
                "orchestra/testbench" : "7.x-dev",
                "doctrine/dbal": "3.7.x"
        },
	"autoload" : {
		"psr-4" : {
			"Sunhill\\Collection\\Tests\\" : [
				"tests",
				"src/Tests"
			],
			"Sunhill\\Collection\\Test\\" : [
				"tests"
			],
			"Sunhill\\Collection\\" : "src"
		}
	},
	"extra" : {
		"laravel" : {
			"providers" : [
				"Sunhill\\Collection\\CollectionServiceProvider"
			]
		}
	}
}
